  "scripts": {
    "start": "(rm -r src/less || true ) && (cp themes.config.js src/themes.config.js) && NODE_OPTIONS=--openssl-legacy-provider react-app-rewired start",
    "build": "(rm -r src/less || true ) && (cp themes.config.js src/themes.config.js) && NODE_OPTIONS=--openssl-legacy-provider react-app-rewired build",
    "test": "(rm -r src/less || true ) && (cp themes.config.js src/themes.config.js) && NODE_OPTIONS=--openssl-legacy-provider react-app-rewired test --env=jsdom",
    "eject": "react-app-rewired eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!src/components/**/index.js",
      "!src/**/*.dev.js",
      "!/node_modules/",
      "!src/index.js",
      "!src/serviceWorker.js"
    ]
  },
  "devDependencies": {
    "jest-react-hooks-shallow": "^1.4.1"
  }
